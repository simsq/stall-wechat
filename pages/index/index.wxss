.container-layout {
  width: 100%;
  height: 300px;
}

.bottom-layout {
  width: 100%;
}

.map-wrapper {
  width: 100%;
  position: relative;
}

.map {
  height: 100%;
  width: 100%;
}

.cover-layout {
  width: 73rpx;
  height: 73rpx;
  position: absolute;
  padding-left: 3px;
  padding-bottom: 2px;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.cover-image {
  width: 50px;
  height: 50px;
}

.shangbao {
  width: 82rpx;
  height: 82rpx;
  position: absolute;
  padding-left: 87%;
  padding-bottom: 2px;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
